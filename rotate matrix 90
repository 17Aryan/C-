#include<bits/stdc++.h>
using namespace std;
int main(){
    int a[3][3],i,j,n,m;
    cin>>n>>m;
    cout<<"enter matrix"<<endl;
    for(i=0;i<n;i++){
        for(j=0;j<m;j++){
            cin>>a[i][j];
        }
    }
    for(i=0;i<m;i++){
        for(j=n-1;j>=0;j--){
            cout<<a[j][i];
        }
        cout<<endl;
    }
    return 0;
}
