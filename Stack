#include<bits/stdc++.h>
using namespace std;

int main()
{
    stack<int> stack1;
    stack1.push(1);
    stack1.push(2);
    stack1.push(3);
    stack1.push(4);
    // cout<<stack1.top()<<endl;
    int num=0;
    stack1.push(num);
    stack1.pop();
    stack1.pop();
    stack1.pop();
    
    while(!stack1.empty()){
        cout<<stack1.top()<<" ";
        stack1.pop();
    }
    return 0;
}
