#include<bits/stdc++.h>
using namespace std;

string hill(string s){
    int l=s.length();
    int j=0;
    string s1;
    for(int i=0;i<l;i++){
        if(s[i] != 'a'||s[i] != 'A'||s[i] != 'e'||s[i] != 'E'||s[i] != 'i'||s[i] != 'I'||s[i] != 'o'||s[i] != 'O'||s[i] != 'u'||s[i] != 'U'){
            s1[j].replace(s[i]);
            j++;
        }
    }
    return s1;
    
}

int main(){
    string s;
    cin>>s;
    string c=hill(s);
    cout<<c;
    return 0;
}
